{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "alsofronie/eloquent-uuid": "^1.0",
        "artesaos/seotools": "^0.12.0",
        "aws/aws-php-sns-message-validator": "^1.5",
        "aws/aws-sdk-php-laravel": "^3.1",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "cocur/slugify": "^3.1",
        "cornford/googlmapper": "^2.32",
        "doctrine/cache": "^1.7",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^5.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "ipunkt/laravel-analytics": "^1.4",
        "ixudra/curl": "^6.16",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.0",
        "martijnc/php-csp": "^1.0",
        "mercuryseries/flashy": "^1.0",
        "sammyk/laravel-facebook-sdk": "^3.5",
        "spatie/laravel-permission": "^2.12",
        "unisharp/laravel-ckeditor": "^4.7",
        "unisharp/laravel-filemanager": "v2.0.0-alpha7",
        "watson/sitemap": "^2.0",
        "willvincent/feeds": "^1.1"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "BlaudCMS\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
